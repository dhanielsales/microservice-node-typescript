{
  "name": "curso-node",
  "version": "1.0.0",
  "main": "server.ts",
  "repository": "git@github.com:dhanielsales/typescript-node.git",
  "author": "Dhanielr <dhanielr@hotmail.com>",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "dev:server": "ts-node-dev --watch --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/shared/infra/http/server.ts",
    "dev:server-debug": "ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/shared/infra/http/server.ts",
    "typeorm": "ts-node-dev  -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/typeorm.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "yarn build && node dist/server.js"
  },
  "dependencies": {
    "@prisma/client": "^2.20.1",
    "aws-sdk": "^2.886.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "slugify": "^1.5.0",
    "uuidv4": "^6.2.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.22",
    "@types/readline-sync": "^1.4.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prisma": "2.20.1",
    "prisma-dbml-generator": "^0.7.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.1.3"
  }
}
