{
  "name": "curso-node",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "git@github.com:dhanielsales/typescript-node.git",
  "author": "Dhanielr <dhanielr@hotmail.com>",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "dev:server": "ts-node-dev --watch --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/index.ts",
    "dev:server-debug": "ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/index.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "yarn build && node dist/index.js",
    "migration:create": "db-migrate create --sql-file",
    "migration:run": "db-migrate up",
    "migration:rollback": "db-migrate down",
    "lint": "eslint './src/**/*' --fix"
  },
  "dependencies": {
    "@types/ioredis": "^4.27.1",
    "@types/redis": "^2.8.31",
    "@types/uuid": "^8.3.1",
    "aws-sdk": "^2.886.0",
    "date-fns": "^2.23.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "ioredis": "^4.27.8",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.10",
    "mongodb": "^4.3.1",
    "pg": "^8.7.1",
    "slugify": "^1.5.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.1.3"
  }
}
